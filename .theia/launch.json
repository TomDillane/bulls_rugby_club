    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "/workspace/bulls_rugby_club/app.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0"
